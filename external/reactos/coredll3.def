begin
  //GlobalUnlock:=LocalUnlock(hMem);
  GlobalUnlock:=True; //see localunlock
end;

function GlobalFreePtr(lp:Pointer):Pointer;
begin
  GlobalFreePtr:=Pointer(GlobalFree(HANDLE(GlobalUnlockPtr(lp))));
end;

