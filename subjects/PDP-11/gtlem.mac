	.WORD	LOWY,0
	.WORD	DNUM,0
	.WORD	DSTACK,STACKD
	.WORD	STACKD,DTOP
	.WORD	LPFLG1,0
	.WORD	MOON,.
	.WORD	DPC,ITEME1
	.WORD	LKS,100
